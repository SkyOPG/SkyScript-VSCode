{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "SkyScript",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.ss",
				"match": "\\b(if|while|for|return|else)\\b"
			},
		{
			"name": "keyword.operator.ss",
			"match": "\\b(set|lock|fun)\\b" 
		},
	{
		"name": "markup.quote.ss",
		"match": "\\bnnei\\b"
	},
{
	"name": "entity.other.inherited-class.ss",
	"match": "\\bout\\b"
}]
		},
		"strings": {
			"name": "string.quoted.double.ss",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.ss",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.ss"
}