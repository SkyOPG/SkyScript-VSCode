{
  "name": "skyscript",
  "displayName": "skyscript",
  "description": "SkyScript Language Support for VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ss",
      "aliases": ["SkyScript", "ss"],
      "extensions": [".ss"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ss",
      "scopeName": "source.ss",
      "path": "./syntaxes/ss.tmLanguage.json"
    }]
  }
}
